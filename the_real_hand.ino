////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
#include <SoftwareSerial.h>
#include <Wire.h>
#include "I2Cdev.h"
#include "MPU6050.h"

////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
SoftwareSerial bluetooth (10, 11);
MPU6050 accel;
int xPos;
int yPos;
int zPos;

void setup () {
  Wire.begin ();
  bluetooth.begin (9600);
  accel.initialize ();
  
}

void loop () {
  accel.getAcceleration (&xPos, &yPos, &zPos);
  bluetooth.print (xPos/100); bluetooth.print ("\t");
  bluetooth.println (yPos/100);
}
